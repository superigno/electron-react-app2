{
  "name": "globalfxchoice-config-manager",
  "productName": "Global FxChoice Configuration Manager",
  "version": "1.0.0",
  "description": "Global FxChoice Configuration Manager",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package --arch ia32",
    "make": "electron-forge make --arch ia32",
    "publish": "electron-forge publish --arch ia32",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "gino.q",
    "email": "gino.q@pure-commerce.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "globalfxchoice_config_manager"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/components/App.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/file-saver": "^2.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/xml2js": "^0.4.8",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "css-loader": "^4.3.0",
    "electron": "11.2.3",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^1.2.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "node-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.38.2",
    "@blueprintjs/docs-theme": "^3.9.4",
    "@blueprintjs/labs": "^0.16.3",
    "@blueprintjs/select": "^3.15.4",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "classnames": "^2.2.6",
    "d-forest": "^1.4.2",
    "electron-log": "^4.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "file-saver": "^2.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "xml2js": "^0.4.23",
    "xmlbuilder": "^15.1.1"
  }
}
